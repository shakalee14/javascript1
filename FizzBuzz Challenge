

var number  = prompt("What number would you like to use?"); 

var num = function(number) {

for (var i = 0; i <= number; i++) 
{

if 
	(i%3=== 0 && i%5=== 0) {

	console.log ("FizzBuzz");}

else if
	(i % 3 === 0) 
{
	console.log ("Fizz");
}

else if (i%5 === 0)
{

	console.log ("Buzz");
}

else {
	console.log(i);
}
}
};
num(number);


	
